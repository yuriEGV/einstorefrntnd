{
  "version": 2,
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "https://einstorebcknd.vercel.app/api/$1"
    },
    {
      "source": "/uploads/(.*)",
      "destination": "https://einstorebcknd.vercel.app/uploads/$1"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      },
      "continue": true
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}